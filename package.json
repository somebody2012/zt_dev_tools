{
  "publisher":"diaoxr",
  "name": "zt-dev-tools",
  "displayName": "zt_dev_tools",
  "description": "tools",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "repository": "https://github.com/somebody2012/vscode-plugin.git",
  "icon": "images/tool.png",
  "contributes": {
    "commands": [
      {
        "command": "zt-dev-tools.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "zt-dev-tools.build:trade",
        "title": "build:trade"
      },
      {
        "command": "zt-dev-tools.saveAll",
        "title": "saveAll"
      }
    ],
    "menus": {
      "explorer/context":[
        {
          "command":"zt-dev-tools.build:trade",
          "group":"navigation"
        }
      ]
    },
    "snippets": [
      {
        "language": "javascript",
        "path": "./src/static/snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./src/static/snippets/javascript.json"
      },
      {
        "language": "vue",
        "path": "./src/static/snippets/vue.json"
      }
    ],
    "configuration":{
      "title": "zt_dev_tool_config",
      "properties": {
        "name":{
          "type":"string",
          "default":"jack",
          "description": "名字"
        }
      }
    },
    "keybindings":{
      "command": "zt-dev-tools.saveAll",
      "key": "ctrl+s",
      "mac": "cmd+s",
      "when": "editorTextFocus"
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "pro": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "package":"vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.46.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "ts-loader": "^8.0.0",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "fs-extra": "^9.0.1",
    "mysql": "^2.18.1",
    "node-xlsx": "^0.15.0",
    "ssh2": "^0.8.9",
    "uuid": "^8.2.0"
  }
}
